    .eu-rail-body h3{
      margin:0 0 4px;
      font-size:18px;
      color:#174733;
      font-weight:800
    }
    .eu-rail-body p{
      margin:0;
      color:#55657a
    }
    
    /* Audience Grid */
    .eu-grid{
      display:grid;
      gap:16px;
      grid-template-columns:repeat(auto-fit,minmax(280px,1fr));
      margin-top:18px
    }
    .eu-box{
      background:#fff;
      border:2px solid var(--line);
      border-radius:14px;
      padding:18px
    }
    .eu-box h3{
      margin:0 0 10px;
      color:#174733;
      font-weight:800
    }
    .eu-x li:before{
      content:"×";
      color:#9CA3AF
    }
    
    /* Pricing */
    .eu-price-wrap{
      max-width:680px;
      margin:18px auto;
      background:#fff;
      border:3px solid var(--purple);
      border-radius:20px;
      padding:28px;
      box-shadow:var(--shadow);
      text-align:center;
      position:relative
    }
    .eu-badge{
      display:inline-block;
      background:var(--green);
      color:var(--green-ink);
      font-weight:900;
      padding:6px 14px;
      border-radius:999px;
      margin:-40px 0 12px
    }
    .eu-amount{
      font-size:52px;
      font-weight:900;
      color:#153a2b;
      margin:.1em 0
    }
    .eu-includes{
      text-align:left;
      margin:16px 0
    }
    .eu-guarantee{
      background:#F4FBF7;
      border:2px solid var(--green);
      border-radius:12px;
      padding:14px;
      margin:16px 0;
      text-align:center
    }
    
    /* FAQ */
    .eu-faq{
      background:#F4F1FF;
      border:1px solid #e7e3fb;
      border-radius:16px;
      padding:20px
    }
    .eu-faq details{
      background:#fff;
      border:1px solid var(--line);
      border-radius:12px;
      padding:14px;
      margin:10px 0;
      transition:box-shadow .2s
    }
    .eu-faq details[open]{
      box-shadow:0 4px 12px rgba(0,0,0,.06)
    }
    .eu-faq summary{
      cursor:pointer;
      font-weight:800;
      color:#174733;
      list-style:none
    }
    .eu-faq summary::-webkit-details-marker{
      display:none
    }
    .eu-faq summary:focus{
      outline:3px solid var(--purple);
      outline-offset:3px;
      border-radius:8px
    }
    .eu-faq details p{
      margin:10px 0 0;
      color:#55657a
    }
    
    /* Final CTA */
    .eu-final{
      background:#fff;
      border:1px solid var(--line);
      border-radius:var(--radius);
      padding:clamp(24px,4vw,48px);
      text-align:center;
      box-shadow:var(--shadow)
