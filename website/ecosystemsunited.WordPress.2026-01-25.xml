    .eu-center{
      text-align:center
    }
    
    @media (max-width:720px){
      .eu-rail:before{left:20px}
      .eu-rail-item{grid-template-columns:42px 1fr}
      .eu-rail-dot{width:42px;height:42px;font-size:16px}
